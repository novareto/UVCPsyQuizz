<h1>Ergebnisse</h1>

<div tal:repeat="question view.display()">
  <p>
  <b tal:content="question['title']" />
  <span tal:content="question['description']" />
  </p>
  <ul>
    <li tal:repeat="answer question['answers']">
      <span tal:content="answer['title']" /> :
      <b tal:content="answer['value']" />
      <i tal:content="'(%.2f%%)' % answer['percent']" />
    </li>
  </ul>
</div>
