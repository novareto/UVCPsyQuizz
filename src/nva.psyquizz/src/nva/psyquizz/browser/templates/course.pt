<div tal:define="baseurl view.url(context)">
  <h1 tal:content="context.name" />
  <p>Course starts the <span tal:content="context.startdate" /> and ends the <span tal:content="context.enddate" /></p>
  <h2 tal:content="context.quizz_type" />
  <dl>
    <dt>Abgeschlossene Fragebogen</dt>
    <dd>
      <ul>
	<li tal:repeat="student context.complete">
	  <a tal:attributes="href '%s/%s' % (baseurl, student.access)"
	      tal:content="student.access" />
	</li>
      </ul>
    </dd>
  </dl>
  <dl>
    <dt>Ausstehende Fragebogen</dt>
    <dd>
      <ul>
	<li tal:repeat="student context.uncomplete">
	  <a tal:attributes="href '%s/%s' % (baseurl, student.access)"
	      tal:content="student.access" />
	</li>
      </ul>
    </dd>
  </dl>
  <h2>Criterias</h2>
  <div tal:repeat="crit view.criterias.keys()">
    <h3 tal:content="crit" />
    <ul>
      <li tal:repeat="val view.criterias[crit]"
	  tal:content="val" />
    </ul>
  </div>
</div>
