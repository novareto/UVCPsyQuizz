<div tal:define="baseurl view.url(context)">
  <h1 tal:content="context.name" />
  <h2 tal:content="context.quizz_type" />

  <h3>Sessions</h3>
  <ul>
    <li tal:repeat="session context.sessions">
      <a tal:attributes="href '%s/sessions/%s' % (baseurl, session.id)"
	 tal:content="session.startdate" />
    </li>
  </ul>
 
  <h3>Criterias</h3>
  <div tal:repeat="crit view.criterias.keys()">
    <h3 tal:content="crit" />
    <ul>
      <li tal:repeat="val view.criterias[crit]"
	  tal:content="val" />
    </ul>
  </div>
</div>
